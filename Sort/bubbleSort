#include <stdio.h>
void main() {
	int list[] = { 69,19,39,29,16,3,21,1 };
	int tmp ,j, i = 0;
	int len;
	for(j =7; j>0;j--){
	for (i = 0; i < j; i++) {
		if (list[i] > list[i + 1]) {
			tmp = list[i];
			list[i] = list[i + 1];
			list[i + 1] = tmp;
		}
	}
	printf("%d회전 : ", 8-j);
	for (i = 0; i < 8; i++)
		printf("%4d", list[i]);
	printf("\n");
	}
